<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd" name="uuf-unit-archetype"
    xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <fileSets>
        <fileSet filtered="true" encoding="UTF-8">
            <directory>docs</directory>
            <includes>
                <include>**/*.txt</include>
            </includes>
        </fileSet>
        <fileSet filtered="true" encoding="UTF-8">
            <directory></directory>
            <excludes>
                <exclude>*.iml</exclude>
                <exclude>**/.idea/**</exclude>
            </excludes>
            <includes>
                <include>resource.xml</include>
                <include>.gitignore</include>
                <include>unit_name.js</include>
                <include>unit_name.json</include>
                <include>unit_name.hbs</include>
            </includes>
        </fileSet>
        <fileSet encoding="UTF-8">
            <directory>public/less</directory>
        </fileSet>
        <fileSet encoding="UTF-8">
            <directory>public/css</directory>
        </fileSet>
        <fileSet encoding="UTF-8">
            <directory>public/js</directory>
        </fileSet>
        <fileSet encoding="UTF-8">
            <directory>public/img</directory>
        </fileSet>
    </fileSets>
</archetype-descriptor>
